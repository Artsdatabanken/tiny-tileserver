{
  "name": "tiny-tileserver",
  "description": "Tiny raster and vector tile server.",
  "version": "0.0.0-development",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "travis-deploy-once": "^5.0.9",
    "semantic-release": "^15.10.3"
  },
  "scripts": {
    "start": "node tiny-tileserver.js --port 8000 data/",
    "test": "jest --coverage",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@mapbox/vector-tile": "^1.3.1",
    "express": "^4.16.3",
    "log-less-fancy": "^1.4.0",
    "minimist": "^1.2.0",
    "pbf": "^3.1.0",
    "sqlite3": "^4.0.2",
    "zlib": "^1.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Artsdatabanken/tiny-tileserver.git"
  },
  "keywords": [
    "mbtiles",
    "raster-tiles",
    "vector-tiles"
  ],
  "author": "Bjorn Reppen <bjreppen@gmail.com> (https://github.com/bjornreppen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Artsdatabanken/tiny-tileserver/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "homepage": "https://github.com/Artsdatabanken/tiny-tileserver#readme"
}
