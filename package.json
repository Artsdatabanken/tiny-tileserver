{
  "name": "tiny-tileserver",
  "description": "Tiny raster+vector tiles and static file server.",
  "version": "1.3.2",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.8.0"
  },
  "scripts": {
    "start": "node tiny-tileserver.js --port 8000 data/",
    "commit": "npx git-cz",
    "test": "jest --coverage",
    "report-coverage": "coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "@mapbox/vector-tile": "^1.3.1",
    "canvas": "^2.2.0",
    "express": "^4.16.4",
    "log-less-fancy": "^1.4.0",
    "minimist": "^1.2.0",
    "pbf": "^3.2.0",
    "sqlite3": "^4.0.3",
    "zlib": "^1.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Artsdatabanken/tiny-tileserver.git"
  },
  "keywords": [
    "mbtiles",
    "raster-tiles",
    "vector-tiles"
  ],
  "author": "Bjorn Reppen <bjreppen@gmail.com> (https://github.com/bjornreppen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Artsdatabanken/tiny-tileserver/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "homepage": "https://github.com/Artsdatabanken/tiny-tileserver#readme"
}
